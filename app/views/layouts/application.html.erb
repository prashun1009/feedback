<!DOCTYPE html>
<html>
  <head>
    <title>Feedbackform</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= render 'layouts/header' %>
    <div class="container">
      <% flash.each do |message_type, message| %>
         <div class="alert alert-<%= message_type %>">
          <%= message %>
         </div>
      <% end %>
    </div>
   <%= yield %>
  </body>
</html>
